{
    "compilerOptions": 
    {
        "target": "ES2022",
        "module":"CommonJS",
        "sourceMap": true,
        "removeComments": true,
        "strict":true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noImplicitReturns": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleresolution":"Node"
    },
    "exclude":["node_modelues"]
}